<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane prefWidth="1200.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TableController">
   <left>
      <AnchorPane minWidth="480.0" BorderPane.alignment="CENTER_LEFT">
         <children>
            <Label text="Accounts" textFill="#515151">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <ScrollPane fitToHeight="true" fitToWidth="true" layoutY="35.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="62.0">
               <content>
                  <TableView fx:id="accounts" onMouseClicked="#selData" />
               </content>
            </ScrollPane>
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </AnchorPane>
   </left>
   <center>
      <AnchorPane minWidth="-Infinity" prefWidth="300.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="Shows" textFill="#515151">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TabPane tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
              <tabs>
                  <Tab text="Series">
                     <content>
                        <ScrollPane fitToHeight="true" fitToWidth="true">
                           <content>
                              <AnchorPane>
                                 <children>
                                    <TableView fx:id="series" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                       <columnResizePolicy>
                                          <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                       </columnResizePolicy>
                                    </TableView>
                                 </children>
                              </AnchorPane>
                           </content>
                        </ScrollPane>
                     </content>
                  </Tab>
                  <Tab text="Movies">
                     <content>
                        <ScrollPane fitToHeight="true" fitToWidth="true">
                           <content>
                              <AnchorPane>
                                 <children>
                                    <TableView fx:id="movies" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                       <columnResizePolicy>
                                          <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                       </columnResizePolicy>
                                    </TableView>
                                 </children>
                              </AnchorPane>
                           </content>
                        </ScrollPane>
                     </content>
                  </Tab>
              </tabs>
               <padding>
                  <Insets top="35.0" />
               </padding>
            </TabPane>
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </AnchorPane>
   </center>
   <right>
      <AnchorPane minWidth="-Infinity" prefWidth="900.0" BorderPane.alignment="CENTER_RIGHT">
         <children>
            <Label prefHeight="31.0" prefWidth="54.0" text="Profile" textFill="#515151">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <ScrollPane fitToHeight="true" fitToWidth="true" layoutY="35.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="62.0">
               <content>
                  <TableView fx:id="profiles">
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
               </content>
            </ScrollPane>
         </children>
      </AnchorPane>
   </right>
   <bottom>
      <ButtonBar BorderPane.alignment="CENTER">
         <buttons>
            <Button fx:id="refresh" mnemonicParsing="false" onAction="#refresh" text="Refresh" />
         </buttons>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </ButtonBar>
   </bottom>
</BorderPane>
